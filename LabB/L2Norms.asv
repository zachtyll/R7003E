norm73_theta = norm(info73(1:ratio73:end,3), euclidian);
norm93_theta = norm(info93(1:ratio93:end,3), euclidian);
norm113_theta = norm(info113(1:ratio113:end,3), euclidian);
norm133_theta = norm(info133(1:ratio133:end,3), euclidian);
norm153_theta = norm(info153(1:ratio153:end,3), euclidian);
norm173_theta = norm(info173(1:ratio173:end,3), euclidian);
norm193_theta = norm(info193(1:ratio193:end,3), euclidian);
norm200_theta = norm(info200(800:ratio200:end,2), euclidian);

